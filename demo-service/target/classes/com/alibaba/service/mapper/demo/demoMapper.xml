<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.alibaba.service.mapper.demo.DemoMapper">
    <resultMap id="BaseResultMap" type="com.alibaba.model.dto.User">
        <id column="ID" jdbcType="DECIMAL" property="id"/>
        <result column="USERNAME" jdbcType="VARCHAR" property="userName"/>
        <result column="PASSWORD" jdbcType="DECIMAL" property="password"/>
    </resultMap>
    <select id="selectDemoByPk" resultMap="BaseResultMap">
        SELECT * FROM user
    </select>
</mapper>